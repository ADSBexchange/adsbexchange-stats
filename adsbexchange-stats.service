[Unit]
Description=ADSBexchange Stats
After=adsbexchange-go.service

[Service]
Type=forking
ExecStart=/home/pi/adsbexchange/json-status 2>&1 >> /dev/null &
WorkingDirectory=/tmp/
StandardOutput=inherit
StandardError=inherit
Restart=on-failure
RestartSec=30
User=root

[Install]
WantedBy=multi-user.target
